{
  "name": "ad-manager-url-extractor",
  "version": "1.0.0",
  "description": "Automated URL extraction tool for Google Ad Manager's Ad Review Center",
  "main": "puppeteer-integration.js",
  "scripts": {
    "start": "node puppeteer-integration.js",
    "extract": "node puppeteer-integration.js",
    "extract-headless": "node puppeteer-integration.js --headless",
